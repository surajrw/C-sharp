7. Write a C# Sharp program to determine the eligibility for admission to a professional course based on the following criteria.
   static void Main(string[] args)
        {
            int math, phy , che;
            Console.WriteLine("enter math");
            math=Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("enter phy");
            phy=Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("enter che");
            che=Convert.ToInt32(Console.ReadLine());
            if(math>=65)
            {
                if(phy>=55)
                {
                    if(che>=50)
                    {
                        if((math+phy+che)>=180)
                        {
                            Console.WriteLine("You are eligible");
                        }
                    }
                }
            }
            else
            {
                Console.WriteLine("you are not eligible");
            }
            Console.ReadLine();
---------------------------------------------------------------------------------------
8. Write a C# Sharp program to read roll no, name and marks of three subjects and calculate the total, percentage and division.
   static void Main(string[] args)
        {
            int math, phy , che , per,total;
            Console.WriteLine("enter math");
            math=Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("enter phy");
            phy=Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("enter che");
            che=Convert.ToInt32(Console.ReadLine());
            total =math+che+phy;
            per=(total*100)/300;
            Console.WriteLine(per);
            if(per>=80)
            {
                Console.WriteLine("first division");
            }
            else if(per>=60 && per<80)
            {
                Console.WriteLine("second division");
            }
            else
            {
                Console.WriteLine("third division");
            }
            Console.ReadLine();
---------------------------------------------------------------------------------------
9. Write a C# Sharp program to check whether a triangle is Equilateral, Isosceles or Scalene.
   static void Main(string[] args)
        {
            int a,b,c;
            Console.WriteLine("enter a");
            a=Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("enter b");
            b=Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("enter c");
            c=Convert.ToInt32(Console.ReadLine());
            
            if(a==b && b==c)
            {
                Console.WriteLine("equi traingle");
            }
            else if(a==b || a==c || b==c)
            {
                Console.WriteLine("iso traingle");
            }
            else
            {
                Console.WriteLine("Scalene triangle");
            }
            Console.ReadLine();
---------------------------------------------------------------------------------------
10. Vowel or Consonent.
    static void Main(string[] args)
        {
            char ch;
            Console.WriteLine("enter any character");
            ch=Convert.ToChar(Console.ReadLine().ToLower());
            if((ch=='a' )||( ch=='e') || ( ch=='i') || ( ch=='o') || ( ch=='u'))
            {
                Console.WriteLine("vowel");
            }
            else
            {
                Console.WriteLine("consonent");
            }
            Console.ReadLine();
---------------------------------------------------------------------------------------
11. Write a C# Sharp program that calculates the area of geometrical shapes using a menu-driven approach.
     static void Main(string[] args)
        {
            int choice,l,b,h,r;
            double area=0;
            Console.WriteLine("enter 1 for circle");
            Console.WriteLine("enter 2 for rectangle");
            Console.WriteLine("enter 3 for triangle");
            Console.WriteLine("enter choice");
            choice=Convert.ToInt32(Console.ReadLine());
            switch(choice)
            {
                case 1:
                Console.Write("Input radius of the circle : ");
                r = Convert.ToInt32(Console.ReadLine()); 
                area = 3.14 * r * r;  
                Console.WriteLine(area);
                break;
                 
                case 2:
                Console.Write("Input length of the rectangle : ");
                l = Convert.ToInt32(Console.ReadLine()); 
                Console.Write("Input breadth of the rectangle : ");
                b = Convert.ToInt32(Console.ReadLine()); 
                area=l*b;
                Console.WriteLine(area);
                break;
                
                case 3:
                Console.Write("Input height of the triangle : ");
                h = Convert.ToInt32(Console.ReadLine()); 
                Console.Write("Input breadth of the triangle : ");
                b = Convert.ToInt32(Console.ReadLine()); 
                area=0.5*h*b;
                Console.WriteLine(area);
                break;
            }
            Console.ReadLine();
---------------------------------------------------------------------------------------

