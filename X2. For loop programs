   static void Main()
    {
        int i,n,sum=0;
        Console.WriteLine("enter range");
        n=Convert.ToInt32(Console.ReadLine());
        for(i=1;i<=n;++i)
        {   
            if(i%2==0)
            {
                Console.WriteLine(i);
                sum=sum+i;
            } 
        }
        Console.WriteLine(sum);
        Console.ReadLine(); 
